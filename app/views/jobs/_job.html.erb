  <p class="my-5">
    <strong class="block font-medium mb-1">Bedrijf:</strong>
    <%= job.bedrijf %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Titel:</strong>
    <%= job.titel %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Keywords:</strong>
    <%= job.keywords %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">H1:</strong>
    <%= job.h1 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Subh1:</strong>
    <%= job.subh1 %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= job.content %>
  </p>

  <p class="my-5">
