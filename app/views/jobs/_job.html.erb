    <strong class="block font-medium mb-1">Talen:</strong>
    <%= job.talen %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Utm:</strong>
    <%= job.utm %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Link:</strong>
    <%= job.link %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this job", job, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this job', edit_job_path(job), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
